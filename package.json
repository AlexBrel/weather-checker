{
  "name": "weather-checker",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "set NODE_ENV=dev&& npm run cleanup && webpack --config config/webpack.config.js --display-error-details",
    "build:prod": "set NODE_ENV=prod&& npm run cleanup && npm run compile && webpack --config config/webpack.config.js",
    "cleanup": "rimraf src/**/*.ngfactory.ts src/**/*.ngstyle.ts src/**/*.ngsummary.json src/**/*.js src/**/*.js.map build dist",
    "compile": "node_modules/.bin/ngc -p tsconfig.json",
    "dev-server": "webpack-dev-server --progress --port 8090 --config config/webpack.config.js",
    "start": "npm run start:dev",
    "start:dev": "set NODE_ENV=dev&& npm run dev-server",
    "start:prod": "set NODE_ENV=prod&& npm run cleanup && npm run compile && npm run dev-server"
  },
